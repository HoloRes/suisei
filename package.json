{
	"name": "suisei-bot",
	"version": "3.0.0",
	"description": "A custom bot for the Hololive Resort Discord",
	"main": "dist/index.js",
	"scripts": {
		"start": "node dist/index.js",
		"test": "echo \"Error: no test specified\" && exit 1",
		"lint": "eslint \"./src/**\"",
		"lint:ci": "eslint \"./src/**\" ;exit 0",
		"lint:fix": "eslint --fix \"./src/**\"",
		"build": "tsc -p tsconfig.production.json",
		"dev:run": "node-dev dist/index.js",
		"watch": "tsc --watch",
		"dev": "run-p watch dev:run",
		"prepare": "husky install",
		"db:push": "prisma-import -f && prisma db push",
		"db:migrate": "prisma-import -f && prisma migrate dev",
		"db:generate": "prisma-import -f && prisma generate"
	},
	"engines": {
		"node": "^18",
		"pnpm": ">=8"
	},
	"dependencies": {
		"@holores/bansapi.js": "^1.0.0",
		"@holores/holodex": "^1.2.3",
		"@prisma/client": "^5.0.0",
		"@sapphire/decorators": "^6.0.2",
		"@sapphire/discord.js-utilities": "^7.0.1",
		"@sapphire/framework": "4.5.1",
		"@sapphire/pieces": "^3.7.0",
		"@sapphire/plugin-api": "^5.1.0",
		"@sapphire/plugin-logger": "^3.0.5",
		"@sapphire/plugin-scheduled-tasks": "^7.1.1",
		"@sapphire/plugin-subcommands": "^4.0.2",
		"@sapphire/stopwatch": "^1.5.0",
		"@sentry/integrations": "^7.60.0",
		"@sentry/node": "^7.60.0",
		"@sentry/tracing": "^7.60.0",
		"@suiseis-mic/sapphire-modules": "1.0.5",
		"bufferutil": "^4.0.7",
		"bullmq": "^4.6.0",
		"discord.js": "^14.11.0",
		"express": "^4.18.2",
		"flagsmith-nodejs": "^3.0.1",
		"humanize-duration": "^3.29.0",
		"meilisearch": "^0.33.0",
		"parse-duration": "^1.1.0",
		"prisma": "^5.0.0",
		"prisma-import": "^1.0.4",
		"prom-client": "^14.2.0",
		"reflect-metadata": "^0.1.13",
		"search-query-parser": "^1.6.0",
		"string-template": "^1.0.0",
		"tslib": "^2.6.0",
		"typia": "^4.1.7",
		"utf-8-validate": "^6.0.3",
		"winston": "^3.10.0",
		"winston-loki": "^6.0.6",
		"zlib-sync": "^0.1.8"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.7",
		"@commitlint/config-conventional": "^17.6.7",
		"@sentry/types": "^7.60.0",
		"@tsconfig/node18": "^18.2.0",
		"@types/express": "^4.17.17",
		"@types/humanize-duration": "^3.27.1",
		"@types/node": "^20.4.3",
		"@types/string-template": "^1.0.2",
		"@typescript-eslint/eslint-plugin": "^6.1.0",
		"@typescript-eslint/parser": "^6.1.0",
		"eslint": "^8.45.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.3",
		"node-dev": "^8.0.0",
		"npm-run-all": "^4.1.5",
		"ts-patch": "3.0.2",
		"ttypescript": "1.5.15",
		"typescript": "^5.1.6"
	},
	"prisma": {
		"import": {
			"schemas": "prisma/schemas/*.prisma",
			"output": "prisma/schema.prisma"
		}
	},
	"lint-staged": {
		"*.{js,ts,jsx,tsx}": [
			"eslint --fix"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://bitbucket.org/holores/suisei-bot.git"
	},
	"author": "Hololive Resort",
	"license": "MIT",
	"bugs": {
		"url": "https://bitbucket.org/holores/suisei-bot/issues"
	},
	"homepage": "https://bitbucket.org/holores/suisei-bot"
}

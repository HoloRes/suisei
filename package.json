{
	"name": "suisei-bot",
	"version": "3.0.0",
	"description": "A custom bot for the Hololive Resort Discord",
	"main": "dist/index.js",
	"engines": {
		"node": "^18",
		"pnpm": ">=8"
	},
	"dependencies": {
		"@holores/holodex": "^1.2.2",
		"@prisma/client": "^4.12.0",
		"@sapphire/decorators": "^6.0.1",
		"@sapphire/discord.js-utilities": "^6.0.5",
		"@sapphire/framework": "4.4.0",
		"@sapphire/pieces": "^3.6.2",
		"@sapphire/plugin-api": "^5.0.3",
		"@sapphire/plugin-logger": "^3.0.3",
		"@sapphire/plugin-scheduled-tasks": "^6.0.1",
		"@sapphire/plugin-subcommands": "^4.0.1",
		"@sapphire/stopwatch": "^1.5.0",
		"@sentry/integrations": "^7.48.0",
		"@sentry/node": "^7.48.0",
		"@sentry/tracing": "^7.48.0",
		"@suiseis-mic/sapphire-modules": "1.0.5",
		"bufferutil": "^4.0.7",
		"bullmq": "^3.10.4",
		"colorette": "^2.0.19",
		"discord.js": "^14.9.0",
		"execa": "^7.1.1",
		"express": "^4.18.2",
		"flagsmith-nodejs": "^2.5.2",
		"meilisearch": "^0.32.3",
		"node-schedule": "^2.1.1",
		"prisma": "^4.12.0",
		"prom-client": "^14.2.0",
		"reflect-metadata": "^0.1.13",
		"search-query-parser": "^1.6.0",
		"socket.io": "^4.6.1",
		"string-template": "^1.0.0",
		"tslib": "^2.5.0",
		"typia": "^3.7.5",
		"utf-8-validate": "^6.0.3",
		"winston": "^3.8.2",
		"winston-loki": "^6.0.6",
		"zlib-sync": "^0.1.8",
		"prisma-import": "^1.0.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.1",
		"@commitlint/config-conventional": "^17.6.1",
		"@sentry/types": "^7.48.0",
		"@tsconfig/node18": "^1.0.1",
		"@types/express": "^4.17.17",
		"@types/humanize-duration": "^3.27.1",
		"@types/node": "^18.15.11",
		"@types/node-schedule": "^2.1.0",
		"@types/string-template": "^1.0.2",
		"@typescript-eslint/eslint-plugin": "^5.58.0",
		"@typescript-eslint/parser": "^5.58.0",
		"eslint": "^8.38.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"husky": "^8.0.0",
		"lint-staged": "^13.2.1",
		"node-dev": "^8.0.0",
		"npm-run-all": "^4.1.5",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"ts-patch": "3.0.0-beta3",
		"ttypescript": "1.5.15",
		"typescript": "^5.0.4"
	},
	"scripts": {
		"start": "node dist/index.js",
		"test": "echo \"Error: no test specified\" && exit 1",
		"lint": "eslint \"./src/**\"",
		"lint:ci": "eslint \"./src/**\" ;exit 0",
		"lint:fix": "eslint --fix \"./src/**\"",
		"build": "tspc -p tsconfig.production.json",
		"dev:run": "node-dev dist/index.js",
		"watch": "tspc --watch",
		"dev": "run-p watch dev:run",
		"prepare": "ts-patch install -s && husky install",
		"db:push": "prisma-import -f && prisma db push",
		"db:migrate": "prisma-import -f && prisma migrate dev",
		"db:generate": "prisma-import -f && prisma generate"
	},
	"prisma": {
		"import": {
			"schemas": "prisma/schemas/*.prisma",
			"output": "prisma/schema.prisma"
		}
	},
	"lint-staged": {
		"*.{js,ts,jsx,tsx}": [
			"eslint --fix"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://bitbucket.org/holores/suisei-bot.git"
	},
	"author": "Hololive Resort",
	"license": "MIT",
	"bugs": {
		"url": "https://bitbucket.org/holores/suisei-bot/issues"
	},
	"homepage": "https://bitbucket.org/holores/suisei-bot"
}

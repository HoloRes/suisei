		"winston": "^3.8.1",
		"winston-loki": "^6.0.6",
		"zlib-sync": "^0.1.7"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.3",
		"@commitlint/config-conventional": "^17.0.3",
		"@sentry/types": "^7.11.1",
		"@tsconfig/node16": "^1.0.3",
		"@types/express": "^4.17.13",
		"@types/humanize-duration": "^3.27.1",
		"@types/ioredis": "^4.28.10",
		"@types/node": "^18.7.13",
		"@types/node-schedule": "^2.1.0",
		"@types/string-template": "^1.0.2",
		"@typescript-eslint/eslint-plugin": "^5.34.0",
		"@typescript-eslint/parser": "^5.34.0",
		"eslint": "^8.22.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-import-resolver-typescript": "^3.5.0",
		"eslint-plugin-import": "^2.26.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"node-dev": "^7.4.3",
		"npm-run-all": "^4.1.5",
		"prisma": "^4.3.1",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"ttypescript": "1.5.13",
		"typescript": "^4.7.4"
	},
	"scripts": {
		"start": "node dist/index.js",
		"test": "echo \"Error: no test specified\" && exit 1",
		"lint": "eslint \"./src/**\"",
		"lint:ci": "eslint \"./src/**\" ;exit 0",
		"lint:fix": "eslint --fix \"./src/**\"",
		"build": "ttsc -p tsconfig.production.json",
		"dev:run": "node-dev dist/index.js",
		"watch": "ttsc --watch",
		"dev": "run-p watch dev:run",
		"prepare": "npx husky install && node .husky/link.js",
		"db:push": "aurora && prisma db push",
		"db:migrate": "aurora && prisma migrate dev",
		"db:generate": "aurora && prisma generate"
	},
	"lint-staged": {
		"*.{js,ts,jsx,tsx}": [
			"eslint --fix"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://bitbucket.org/holores/suisei-bot.git"
	},
	"author": "Hololive Resort",
	"license": "MIT",
	"bugs": {
		"url": "https://bitbucket.org/holores/suisei-bot/issues"
	},

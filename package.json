{
	"name": "suisei-bot",
	"version": "3.0.0",
	"description": "A custom bot for the Hololive Resort Discord",
	"main": "dist/index.js",
	"engines": {
		"node": "^16",
		"pnpm": ">=3"
	},
	"dependencies": {
		"@holores/holodex": "^1.2.2",
		"@prisma/client": "^4.3.1",
		"@sapphire/decorators": "^5.0.0",

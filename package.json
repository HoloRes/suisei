{
  "name": "suisei-bot",
  "version": "3.0.0",
  "description": "A custom bot for the Hololive Resort Discord",
  "main": "src/index.ts",
  "engines": {
    "node": "^16",
    "pnpm": ">=3"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "@sapphire/decorators": "^4.3.4",
    "@sapphire/discord.js-utilities": "^4.9.5",
    "@sapphire/framework": "3.0.0-next.75c539d.0",
    "@sapphire/pieces": "^3.3.1",
    "@sapphire/plugin-api": "^3.2.1",
    "@sapphire/plugin-subcommands": "^2.2.0",
    "@sentry/integrations": "^6.19.6",
    "@sentry/node": "^6.19.6",
    "@sentry/tracing": "^6.19.6",
    "@suiseis-mic/sapphire-modules": "1.0.5",
    "bufferutil": "^4.0.6",
    "discord.js": "^13.6.0",
    "execa": "^6.1.0",
    "flagsmith-nodejs": "^1.1.2",
    "node-schedule": "^2.1.0",
    "prisma-aurora": "^1.3.6",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.4.1",
    "tslib": "^2.4.0",
    "typescript-is": "^0.19.0",
    "utf-8-validate": "^5.0.9",
    "winston": "^3.7.2",
    "winston-loki": "^6.0.5",
    "zlib-sync": "^0.1.7",
    "prisma": "^3.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@sentry/types": "^6.19.6",
    "@tsconfig/node16": "^1.0.2",
    "@types/express": "^4.17.13",
    "@types/humanize-duration": "^3.27.1",
    "@types/node": "^17.0.25",
    "@types/node-schedule": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.0",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "ttypescript": "1.5.13",
    "typescript": "^4.6.3"
  },
  "scripts": {
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint \"./src/**\"",
    "lint:ci": "eslint \"./src/**\" ;exit 0",
    "lint:fix": "eslint --fix \"./src/**\"",
    "build": "ttsc -p tsconfig.production.json",
    "dev": "ts-node-dev -C ttypescript src/index.ts",
    "prepare": "npx husky install && node .husky/link.js",
    "db:push": "aurora && prisma db push",
    "db:migrate": "aurora && prisma migrate dev",
    "db:generate": "aurora && prisma generate"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/holores/suisei-bot.git"
  },
  "author": "Hololive Resort",
  "license": "MIT",
  "bugs": {
    "url": "https://bitbucket.org/holores/suisei-bot/issues"
  },
  "homepage": "https://bitbucket.org/holores/suisei-bot"
}

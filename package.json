		"@sapphire/plugin-subcommands": "^3.1.1",
		"@sapphire/stopwatch": "^1.4.1",
		"@sentry/integrations": "^7.11.1",
		"@sentry/node": "^7.11.1",
		"@sentry/tracing": "^7.11.1",
		"@suiseis-mic/sapphire-modules": "1.0.5",
		"bufferutil": "^4.0.6",
		"bullmq": "^1.89.2",
		"colorette": "^2.0.19",
		"discord.js": "^13.10.3",
		"execa": "^6.1.0",
		"express": "^4.18.1",
		"flagsmith-nodejs": "^2.2.0",
		"meilisearch": "^0.27.0",
		"node-schedule": "^2.1.0",

// This file holds the core database models required to run the bot

model Server {
    id               String   @id
    prefix           String?
    disabledCommands String[]
    updateChannel    String?
    staffRole        String?
    adminRole        String?
}

model ConfigValue {
    guildId String
    module  String
    key     String
    value   String

    @@id([guildId, module, key])
}

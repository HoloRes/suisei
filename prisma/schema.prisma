// ◮◮◮ GENERATED BY AURORA ◮◮◮
datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Server {
  id               String   @id
  prefix           String?
  disabledCommands String[]
  updateChannel    String?
  staffRole        String?
  adminRole        String?
}

model YoutubeSubscription {
  id       String   @id
  channels String[]
}

model Livestream {
  id         String   @id
  title      String
  messageIds String[]
}
  "compilerOptions": {
	"outDir": "./dist",
	"target": "es6",
	"typeRoots": ["./node_modules/@types", "./src/types"],
	"resolveJsonModule": true,
	"plugins": [
	  { "transform": "typescript-is/lib/transform-inline/transformer" }
	]
  },
  "include": [
	"./src/**/*"
  ],
  "extends": "@tsconfig/node14/tsconfig.json"
}

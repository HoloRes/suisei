{
	"compilerOptions": {
		"outDir": "./dist",
		"typeRoots": [
			"./node_modules/@types",
			"./src/types"
		],
		"resolveJsonModule": true,
		"plugins": [
			{
				"transform": "typescript-is/lib/transform-inline/transformer"
			}
		],
		"strict": true
	},
	"include": [
		"./src/**/*"
	],
	"extends": "@tsconfig/node14/tsconfig.json"
}
